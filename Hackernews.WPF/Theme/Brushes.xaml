<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="skinBackgroundLight">#373547</Color>
    <SolidColorBrush x:Key="skinBackgroundLightBrush"
                     Color="{StaticResource skinBackgroundLight}" />

    <Color x:Key="skinBackground">#272537</Color>
    <SolidColorBrush x:Key="skinBackgroundBrush"
                     Color="{StaticResource skinBackground}" />
    
    
    <Color x:Key="skinBackgroundDark">#222030</Color>
    <SolidColorBrush x:Key="skinBackgroundDarkBrush"
                     Color="{StaticResource skinBackgroundDark}" />
    
    <Color x:Key="skinFontColor">White</Color>
    <SolidColorBrush x:Key="skinFontBrush"
                     Color="{StaticResource skinFontColor}" />

    <!--Color="#FF838383"-->
    <Color x:Key="skinFontColorSecondary">#CECECE</Color>
    <SolidColorBrush x:Key="skinFontSecondaryBrush"
                     Color="{StaticResource skinFontColorSecondary}" />

    <LinearGradientBrush x:Key="cardBackground"
                         StartPoint="0,0"
                         EndPoint="1, 2">
        <GradientStop Color="#2d2b40" />
        <GradientStop Color="#2b293d"
                      Offset="1" />
    </LinearGradientBrush>
</ResourceDictionary>